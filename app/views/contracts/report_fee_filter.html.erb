<%= bootstrap_form_tag url: 'report_fee_action', method: :get  do |f| %>
  <div class='title'> Relatório de Comissões </div>
  
  <div class='section'>
		<td><%= f.select :month, @months, label: 'Mes' %></td>
    <td><%= f.select :year, @years, label: 'Ano'  %></td>

    <div class='buttonbar'>
      <%= f.primary 'Gerar Relatório' %>
      <%= 'ou' %>
      <%= link_to t('menu.cancel'), root_path %>
    </div>
  </div>
<% end %>
